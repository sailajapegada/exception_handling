try:
  a=int(input("Enter the 1st number"))
  b=int(input("\nEnter the 2nd number"))
  res=a/b
except ValueError:
  print("\nOnly whole numbers can be entered")
except ZeroDivisionError:
  print("\nYou sretrying to divide by 0,not allowed")
else:
  print(F"{a}/{b}+{res}")
finally:
  print("Program completed")
  
  
  
